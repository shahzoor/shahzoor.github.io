	pages = {119--131},
	file = {Guest et al. - 2019 - Gerrymandering and computational redistricting.pdf:/Users/shahzoorsafdar/Zotero/storage/Z8JM53PK/Guest et al. - 2019 - Gerrymandering and computational redistricting.pdf:application/pdf},
}

