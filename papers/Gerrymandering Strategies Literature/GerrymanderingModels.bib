@article{friedman_optimal_2008,
	title = {Optimal {Gerrymandering}: {Sometimes} {Pack}, but {Never} {Crack}},
	volume = {98},
	issn = {0002-8282},
	shorttitle = {Optimal {Gerrymandering}},
	url = {https://www.jstor.org/stable/29729966},
	abstract = {Standard intuitions for optimal gerrymandering involve concentrating one's extreme opponents in "unwinnable" districts ("packing") and spreading one's supporters evenly over "winnable" districts ("cracking"). These intuitions come from models with either no uncertainty about voter preferences or only two voter types. In contrast, we characterize the solution to a problem in which a gerrymanderer observes a noisy signal of voter preferences from a continuous distribution and creates N districts of equal size to maximize the expected number of districts she wins. Under mild regularity conditions, we show that cracking is never optimal—one's most ardent supporters should be grouped together. Moreover, for sufficiently precise signals, the optimal solution involves creating a district that matches extreme "Republicans" with extreme "Democrats," and then continuing to match toward the center of the signal distribution.},
	number = {1},
	urldate = {2022-11-11},
	journal = {The American Economic Review},
	author = {Friedman, John N. and Holden, Richard T.},
	year = {2008},
	note = {Publisher: American Economic Association},
	keywords = {Have read},
	pages = {113--144},
	file = {JSTOR Full Text PDF:/Users/shahzoorsafdar/Zotero/storage/J8WGMGZ2/Friedman and Holden - 2008 - Optimal Gerrymandering Sometimes Pack, but Never .pdf:application/pdf},
}

